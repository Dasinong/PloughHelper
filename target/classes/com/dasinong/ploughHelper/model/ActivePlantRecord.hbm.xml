<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.dasinong.ploughHelper.model.ActivePlantRecord" table="field" catalog="ploughHelper">
        <id name="id" type="java.lang.Long">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="fieldId" type="string">
            <column name="fieldId" length="30" not-null="true" unique="true" />
        </property>
        <property name="startdate" type="string">
            <column name="startdate" not-null="true" />
        </property>
