<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.dasinong.ploughHelper.model.QualityItem" table="qualityItem" catalog="ploughHelper">
        <id name="qualityItemId" type="java.lang.Long">
            <column name="qualityItemId" />
            <generator class="identity" />
        </id>
        <property name="qualityItemName" type="string">
            <column name="qualityItemName" length="30" not-null="true" unique="true" />
        </property>
        <many-to-one name="crop" class="com.dasinong.ploughHelper.model.Crop" fetch="select">
        	<column name="cropId" not-null="true"></column>
        </many-to-one>
    </class>
</hibernate-mapping>