<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.dasinong.ploughHelper.model.Field" table="field" catalog="ploughHelper">
        <id name="fieldId" type="java.lang.Long">
            <column name="fieldId" />
            <generator class="identity" />
        </id>
        <property name="fieldName" type="string">
            <column name="fieldName" length="30" not-null="true" unique="true" />
        </property>
        <property name="other" type="string">
            <column name="other" length="100" not-null="true" unique="true" />
        </property>
    </class>
</hibernate-mapping>