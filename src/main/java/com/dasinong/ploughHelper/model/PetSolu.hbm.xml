<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.dasinong.ploughHelper.model.PetSolu" table="petSolu" catalog="ploughHelper">
        <id name="petSoluId" type="java.lang.Long">
            <column name="petSoluId" />
            <generator class="identity" />
        </id>
        <property name="petSoluName" type="string">
            <column name="petSoluName" length="30" not-null="true" unique="true" />
        </property>
        <many-to-one name="petDisSpec" class="com.dasinong.ploughHelper.model.PetDisSpec" fetch="select">
        	<column name="petDisSpecId" not-null="true"></column>
        </many-to-one>
        <property name="cure" type="java.lang.Boolean">
            <column name="cure" />
        </property>
        <property name="other" type="string">
            <column name="other" length="100" not-null="true" unique="true" />
        </property>
    </class>
</hibernate-mapping>