<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.dasinong.ploughHelper.model.UserPetDis" table="userPetDis" catalog="ploughHelper">
        <id name="id" type="java.lang.Long">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="disType" type="string">
            <column name="disType" length="30"/>
        </property>
        <property name="petName" type="string">
            <column name="petName" length="30"/>
        </property>
        <property name="infectionPart" type="string">
            <column name="infectionPart" length="30"/>
        </property>
        <property name="impactTime" type="string">
            <column name="impactTime" length="30"/>
        </property>
        <property name="impactDis" type="string">
            <column name="impactDis" length="30"/>
        </property>
        <property name="operation" type="string">
            <column name="operation" length="100"/>
        </property>
    </class>
</hibernate-mapping>